//    cmd git     
//  publier repo local  sur le repo distant de GitHub  
            à url = "https://github.com/garaixm/cvdev  " 
                 
$ git config --global user.name " garaixm "  
$ git config --global user.email " XXX "  
$ git add . 
$ git commit -m "création de CV Michel Garaix commentaires"
$ git status   
$ git log --oneline
$ git branch -M master
$ git remote add repdist "https://github.com/UUU/RDIST.git"  -- URL.git
$ git remote -v
 
 --  TOKEN    ---  Settings\Developper settings\Personal access tokens \ generate > TOKEN

$ git remote set-url repdist "https://garaixm:TOKEN@repvw.git"
$ git push repdist master  –- vers repository distant

$ git status
$ git log
/////////////////////////////////////////////////////////////////////////////////////////////////
                 UUU= garaixm  RDIST= cvdev  		
                 repw = github.com/UUU/RDIST      
                   ---  github.com/garaixm/cvdev   
                 URL= https://repw
                     = https://github.com/UUU/RDIST 
                  ---  https://github.com/garaixm/cvdev    
		 URLn = https://UUU:TOKEN@repvw
                      = https://UUU:TOKEN@github.com/UUU/RDIST  
                    -- https://garaixm:TOKEN@github.com/garaixm/cvdev   
		 
          --  $ git remote add repdist URL.git  sinon  $ git remote set-url repdist URLn.git 
                                            

          
